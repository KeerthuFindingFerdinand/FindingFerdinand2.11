    color: white !important;

  }
}




/*============================================================================
Color Guide
==============================================================================*/

.template-blog{

    .breadcrumb{display: none;}

    .information-sub{
      height: 420px;
      overflow: hidden;
    }

    .author-name{
      margin-bottom: 1em;
    }

    @include at-query ($min, $large){

      .site-header{background-color: rgba(254,214,215,0.5);}
      #PageContainer, .main-content{
        padding-top: 0px;
        margin-top: 0px;
      }
    }

    h1.pink-bg{
      background-color: rgba(254,214,215,0.5);
      padding-top: 44px;
      padding-bottom: 1em;
    }

    h1.overline span{
      padding-top: 0.5em;
      border-top: 8px $blue solid;
      position: relative;
      font-size: 125%;
      top: 68px;
    }

    /* All Articles */

    .article{

      h2{
        @include at-query ($min, $large){
          font-size: 3em;
          width: 95%;
          position: relative;
          top: -8px;
          a{
              transition: all 300ms cubic-bezier(0.57, 0.06, 0.05, 0.95);
          }
        }
      }

      .cta{
        display: block;
        background-color: $black;
        color: $white;
        text-align: center;
        text-transform: uppercase;
        max-width: 220px;
        padding-top: 1em;
        padding-bottom: 1em;
        transition: all 300ms cubic-bezier(0.57, 0.06, 0.05, 0.95);
        &:hover, &:focus{
          background-color: $pink;
          color: $black;
        }

        @include at-query ($max, $large){
          margin-left: auto;
          margin-right: auto;
        }
      }

      .excerpt{
        font-size: 1.125em;
        text-align: justify;
        margin: 1.5em 0;
      }
    }


    /* Featured Article */

    .featured-article{

      .article__featured-image{
        display: block;
        height: 0px;
        width: 100%;
        padding-bottom: 100%;
        background-size: cover;
        background-position: center center;

        transition: background-size 300ms cubic-bezier(0.57, 0.06, 0.05, 0.95);

        @include at-query ($min, $large){
          padding: 0;
          height: calc(100vh - 320px);
        }
      }



    }



    /* First Sticker Container */
    .blog-wrap{
      .sticker-wrap{
        height: 0px;
        padding-bottom: 100%;

        @include at-query ($min, $large){
          padding-bottom: 18%;
        }

      }

      .sticker-container{
        position: absolute;
        width: 100%;

        .scale-down{
          width: 330px;
          height: 191px;
          display: block;
          margin-left: auto;
          margin-right: auto;
          position: relative;
          margin-top: ($gutter * 2);

        }

      }
  }




  /* Other Articles */

  .other-articles{
    .article__featured-image{
      display: block;
      height: 0px;
      padding-bottom: 100%;
      background-position: center center;
      background-size: cover;
      background-repeat: no-repeat;
      margin-bottom: $gutter;
    }

    h3, .excerpt-container{
      padding-left: $gutter;
      padding-right: $gutter;
    }

    .sidelines{
      padding-bottom: $gutter;
    }

    .cta{
      display: inline-block; width: 320px; margin-top: 1em; margin-bottom: 1em;
    }
  }



}



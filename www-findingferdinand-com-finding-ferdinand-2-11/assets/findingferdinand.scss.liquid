    background-size: cover;

    .url-block{display: none;}

    &.bump{
      @include transform(scale(1.1225));
    }

    &.selected{
      cursor: pointer;
      transition: all 300ms cubic-bezier(0.57, 0.06, 0.05, 0.95);
      
      &:active{background-color: $black; background-image: none !important;}
      /*.url-block{
        display: block;
        width: 100%;
        height: 100%;
        position: absolute;
        left: 0px;
        top: 0px;
      }*/

      &:hover, &:focus{
        /*background-size: 125%;*/
      }
        /*&:before{
          color: #fff;
          content: "View Details";
          text-align: center;
          font-size: 0.65rem;
          top: 75%;
          width: 66.6%;
          position: absolute;
          z-index: 2;
          left: 50%;
          margin-left: -33.3%;
          line-height: 1;
          text-transform: uppercase;
          font-size: 0.65rem;
          transform: translateY(-50%);
      }
      }*/

    }

    &::after{
      position: absolute;
      width: 16px;
      height: 16px;
      line-height: 16px;
      text-align: center;
      top: 50%;
      left: 50%;
      margin-left: -8px;
      margin-top: -8px;
      font-weight: bold;
      font-size: 0.85rem;
      color: #dcdcdc;
    }



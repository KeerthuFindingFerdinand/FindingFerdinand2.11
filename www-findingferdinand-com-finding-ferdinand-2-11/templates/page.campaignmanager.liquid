 var ref1 = database.ref('posts'); 
 ref1.on('value',gotData,errData); 
 
 
 function gotData(data){ 
  
 var response = data.val(); 
   
 var key = Object.keys(response); 
 console.log(key.length);
 for(var i = 0; i < key.length; i++){ 
 	var k = key[i]; 
 	var posts = response[k]; 
 	
 	
 	append(k,posts.bodyText1,posts.bodyText2,posts.bodyText3,
           posts.bodyText4,posts.bodyText5,posts.bodyText6,
           posts.titleText1,posts.titleText2,posts.titleText3,
